# frozen_string_literal: true

source "https://rubygems.org"

# gem "rails"

# Windows and JRuby does not include zoneinfo files, so bundle the tzinfo-data gem and associated library.
platforms :windows do
  gem "tzinfo", "~> 1.2"
  gem "tzinfo-data"
end

gem "webrick", "~> 1.7"
gem 'html-proofer'
gem 'jekyll-gist'
gem 'rouge'
gem 'racc'
gem 'terminal-table'
gem 'forwardable-extended'
gem "jekyll", "~> 4.4.1"
